{
	"name": "ntnui-tools",
	"version": "0.2.1",
	"description": "An npm package with a collection of tools for easier utilization of NTNUI API.",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/NTNUI/ntnui-tools.git"
	},
	"bugs": {
		"url": "https://github.com/NTNUI/ntnui-tools/issues"
	},
	"author": {
		"name": "NTNUI Sprint",
		"email": "sprint@ntnui.no",
		"url": "https://ntnui.no"
	},
	"main": "./index.js",
	"exports": {
		".": "./index.js",
		"./customError": "./customError.js"
	},
	"files": [
		"index.d.ts",
		"index.js",
		"customError.d.ts",
		"customError.js"
	],
	"engines": {
		"node": ">=14.0.0"
	},
	"scripts": {
		"prepublish": "npm run build",
		"build": "npm run fix && tsc",
		"prettier": "prettier --check .",
		"eslint": "eslint . --ext ts --ext tsx",
		"check": "eslint . --ext ts --ext tsx && prettier --check .",
		"fix": "prettier --write . && eslint ./src --ext .ts,.tsx --fix"
	},
	"keywords": [
		"ntnui",
		"ntnui-tools",
		"ntnui-client",
		"ntnui-api",
		"ntnui-api-tools",
		"ntnui-api-client",
		"ntnui-api-wrapper"
	],
	"dependencies": {
		"axios": "^1.3.0",
		"dotenv": "^16.0.3"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@typescript-eslint/eslint-plugin": "^5.50.0",
		"@typescript-eslint/parser": "^5.50.0",
		"eslint": "^8.33.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-import": "^2.27.5",
		"prettier": "^2.8.3",
		"typescript": "^4.9.5"
	}
}
